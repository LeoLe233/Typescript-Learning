generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("file:./test.db")
}

model Student{
  id Int @id @default(autoincrement())
  email String @unique
  first_name String
  last_name String
  preferred_name String?
  classes Classes[]
}

model Classes{
  id Int @id @default(autoincrement())
  name 
}